<%-include("partials/header")-%>
<h1>Posts</h1>
<div class="fluid posts-wrapper">

<div class="row">

<% for(var i=0;i<posts.length;i++) { %>
  <div class="col-lg-6 col-md-6 col-sm-12 post">
  <%  if((currentTime-posts[i].time)>86400000){break;}%>
    <img src="<%=posts[i].image%>" alt="image" class="post-image">
    <h3><%=posts[i].text%></h3>



    </div>
<% }%>
  </div>
  </div>




<%-include("partials/footer")-%>
